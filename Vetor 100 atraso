#include <stdio.h>

int main() {
   int vetor[5], count = 0;
   
   // lê 5 números inteiros e adiciona-os ao vetor
   for (int i = 0; i < 5; i++) {
      printf("Digite um número inteiro: ");
      scanf("%d", &vetor[i]);
   }
   
   // percorre o vetor e conta quantos números são maiores que 100
   for (int i = 0; i < 5; i++) {
      if (vetor[i] > 100) {
         count++;
      }
   }
   
   // exibe a quantidade de números maiores que 100
   printf("Existem %d números maiores que 100 no vetor.\n", count);
   
   return 0;
}
